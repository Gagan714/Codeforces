import java.util.*;
import java.lang.*;
public class Solution{
    public static void main(String []args){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int count=0;
        for(int i=0;i<n;i++){
            int sum=0;
            sum+=sc.nextInt();
            sum+=sc.nextInt();
            sum+=sc.nextInt();
            if(sum>=2){
                count++;
            }
        }
        System.out.println(count);
    }
}
