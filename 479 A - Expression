
import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
            int a=sc.nextInt();
            int b=sc.nextInt();
            int c=sc.nextInt();
            int output=Math.max(a*b*c,a+b+c);
            output=Math.max(output,(a+b)*c);
            output=Math.max(output,a*(b+c));
            System.out.println(output); 
    }
}
