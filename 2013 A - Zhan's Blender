import java.util.*;
import java.lang.*;
public class Solution{
    public static void main(String []args){
        Scanner sc=new Scanner(System.in);
        int t=sc.nextInt();
        for(int i=0;i<t;i++){
            int f=sc.nextInt();
            int s=Math.min(sc.nextInt(),sc.nextInt());
            if(f%s==0){
            System.out.println(f/s);
            }else{
                System.out.println((f/s)+1);
            }
        }
    }
}
