import java.util.*;
import java.lang.*;
public class Solution{
    public static void main(String []args){
        Scanner sc=new Scanner(System.in);
        int t=sc.nextInt();
        while(t>0){
            int n=sc.nextInt();
            String str=sc.next();
            Set<String> set=new HashSet<>();
            for(int i=0;i<n-1;i++){
                set.add(str.substring(i,i+2));
            }
            System.out.println(set.size());
            t--;
        }
    }
}
