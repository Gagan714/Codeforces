import java.util.*;
import java.lang.*;
public class Solution{
    public static void main(String []args){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int m=sc.nextInt();
        boolean output=false;
        while(n>0 && m>0){
            n--;
            m--;
            output=!output;
        }
        if(output){
            System.out.println("Akshat");
        }else{
            System.out.println("Malvika");
        }
    }
}
