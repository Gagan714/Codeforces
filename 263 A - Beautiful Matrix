import java.util.*;
import java.lang.*;
public class Solution{
    public static void main(String []args){
        int output=0;
        Scanner sc=new Scanner(System.in);
        for(int i=0;i<5;i++){
            for(int j=0;j<5;j++){
                int num=sc.nextInt();
                if(num==1){
                    output=Math.abs(i-2)+Math.abs(j-2);
                }
            }
        }
        System.out.println(output);
    }
}
